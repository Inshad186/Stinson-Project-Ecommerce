        <script>
          let currentPage = 1;
          const itemsPerPage = 5; // Number of items to display per page
          let originalRows = []; // Store the original unfiltered rows
        
          function paginateTable(filteredRows = null) {
            const rows = filteredRows || originalRows;
            const totalItems = rows.length;
            const totalPages = Math.ceil(totalItems / itemsPerPage);
        
            // Update page info
            document.getElementById('current-page').innerText = currentPage;
            document.getElementById('total-pages').innerText = totalPages;
        
            // Hide all rows
            originalRows.forEach(row => row.style.display = 'none');
        
            // Show rows for the current page
            const start = (currentPage - 1) * itemsPerPage;
            const end = start + itemsPerPage;
        
            for (let i = start; i < end && i < totalItems; i++) {
              rows[i].style.display = '';
            }
        
            // Disable previous button on first page
            document.querySelector('.btn.btn-secondary[onclick="prevPage()"]').disabled = currentPage === 1;
            // Disable next button on last page
            document.querySelector('.btn.btn-secondary[onclick="nextPage()"]').disabled = currentPage === totalPages;
          }
        
          function prevPage() {
            if (currentPage > 1) {
              currentPage--;
              paginateTable();
            }
          }
        
          function nextPage() {
            const totalPages = Math.ceil(originalRows.length / itemsPerPage);
            if (currentPage < totalPages) {
              currentPage++;
              paginateTable();
            }
          }
        
          function searchTable() {
            const searchValue = document.getElementById('search-input').value.toLowerCase();
            const rows = Array.from(originalRows);
        
            const filteredRows = rows.filter(row => {
              const cells = Array.from(row.querySelectorAll('td'));
              return cells.some(cell => cell.innerText.toLowerCase().includes(searchValue));
            });
        
            currentPage = 1;
            paginateTable(filteredRows);
          }
        
          // Initialize pagination on page load
          document.addEventListener('DOMContentLoaded', () => {
            originalRows = Array.from(document.querySelectorAll('tbody tr'));
            paginateTable();
          });
        </script>

        <script>

          async function toggleStatus(userId, button, status) {

            const response = await fetch("/admin/blockUser", {
              method: "POST",
              headers: {
                "Content-Type": "application/json"
              },
              body: JSON.stringify({
                userId: userId,
              })
            });
            const data = await response.json();

            if (data.success) {
              button.innerText = data.userState ? "UNBLOCK" : "BLOCK";
              button.classList.remove("bg-success", "bg-danger");
              button.classList.add(data.userState ? "bg-success" : "bg-danger");

              status.innerText = data.userState ? "INACTIVE" : "ACTIVE";

              const result = await Swal.fire({
                title: "Updated!",
                text: "User data has been updated",
                icon: "success"
              });
            }
          }

        </script>

