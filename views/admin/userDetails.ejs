              </div>
              <div class="table-responsive mt-2">
                <table class="table align-middle mb-0">
                  <thead class="table-light">
                    <tr>
                      <th>USER</th>
                      <th>EMAIL</th>
                      <th>PHONE</th>
                      <th>CREATED DATE</th>
                      <th>VERIFIED</th>
                      <th>STATUS</th>
                      <th>ACTION</th>
                    </tr>
                  </thead>
                  <tbody id="user-table-body">
                    <% for(let i=userData.length-1 ; i>=0 ; i--) { %>
                      <tr>
                        <td>
                          <h6 class="product-name mb-1">
                            <%= userData[i].name %>
                          </h6>
                        </td>
                        <td>
                          <div class="d-flex align-items-center gap-3">
                            <div class="product-info">
                              <h6>
                                <%= userData[i].email %>
                              </h6>
                            </div>
                          </div>
                        </td>
                        <td>
                          <h6>
                            <%= userData[i].mobile %>
                          </h6>
                        </td>
                        <td>
                          <h6>
                            <% const d=new Date(userData[i].CreatedAt); const m=d.toLocaleString('default', {
                              month: 'long' }); const day=d.getDate(); const y=d.getFullYear(); let h=d.getHours(); const
                              min=d.getMinutes(); const ampm=h>= 12 ? 'PM' : 'AM';
                              h = h % 12;
                              h = h ? h : 12; // Convert 0 to 12
                              %>
                              <%= m %> <%= day %>, <%= y %> <%= h %>:<%= min < 10 ? '0' + min : min %> <%= ampm %>
                          </h6>            
                        </td>
                        <td><h6>VERIFIED</h6></td>
                        <td id="<%= userData[i]._id %>">
                          <%= userData[i].is_Block ? 'INACTIVE' : 'ACTIVE' %>
                        </td>
                        <td>
                          <span class="badge bg-<%= userData[i].is_Block ? 'success' : 'danger' %>"
                            onclick="toggleStatus('<%= userData[i]._id %>', this , document.getElementById('<%= userData[i]._id %>'))">
                            <%= userData[i].is_Block ? 'UNBLOCK' : 'BLOCK' %>
                          </span>
                        </td>
                        <td>
                          <div class="d-flex align-items-center gap-3 fs-6">
                            <a href="javascript:;" class="text-primary" data-bs-toggle="tooltip" data-bs-placement="bottom" title="" data-bs-original-title="View detail" aria-label="Views">
