              const data = await response.json();

              if (data.success) {
                button.innerText = data.userState ? "UNBLOCK" : "BLOCK";
                button.classList.remove("bg-success", "bg-danger");
                button.classList.add(data.userState ? "bg-success" : "bg-danger");

                status.innerText = data.userState ? "INACTIVE" : "ACTIVE";

                const result = await Swal.fire({
                  title: "Updated!",
                  text: "User data has been updated",
                  icon: "success"
                });
              }
