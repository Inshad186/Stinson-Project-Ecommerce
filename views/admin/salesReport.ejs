          <div class="container mt-5">
            <h1 class="mb-4">Product Sales Report</h1>
        
            <div class="mb-4">
              <form action="/admin/salesReport" method="GET" class="form-inline">
                <label for="sortBy" class="mr-2">Sort By:</label>
                <select name="sortBy" id="sortBy" class="form-control mr-2">
                  <option value="All Orders">All Orders</option>
                  <option value="Today">Today</option>
                  <option value="Last 7 Days">Last 7 Days</option>
                  <option value="Last 30 Days">Last 30 Days</option>
                  <option value="Custom Date">Custom Date</option>
                </select>
                <input type="date" name="startDate" id="startDate" class="form-control mr-2">
                <input type="date" name="endDate" id="endDate" class="form-control mr-2">
                <button type="submit" class="btn btn-primary mr-2">Filter</button>
                <button type="button" id="downloadExcelBtn" class="btn btn-success mr-2">Download Excel</button>
                <button type="button" id="downloadPdfBtn" class="btn btn-danger">Download PDF</button>
              </form>
            </div>
        
            <% if (totalOrders > 0) { %>
            <div>
              <p>Total Orders: <%= totalOrders %></p>
              <p>Grand Total Amount: <%= grandTotalAmount %></p>
        
              <table class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th>Order Date</th>
                    <th>User Name</th>
                    <th>Order Status</th>
                    <th>Payment Method</th>
                    <th>Subtotal</th>
                    <th>Grand Total</th>
                    <th>Delivery Charge</th>
                    <th>Coupon Discount</th>
                    <th>Offer Applied</th>
                  </tr>
                </thead>
                <tbody>
                  <% orders.forEach(order => { %>
                    <tr>
                      <td><%= order.orderDate.toDateString() %></td>
                      <td><%= order.userId.name %></td>
                      <td><%= order.orderItems[0].orderStatus%></td>
                      <td><%= order.paymentMethod %></td>
                      <td><%= order.subTotal %></td>
                      <td><%= order.grandTotal %></td>
                      <td><%= order.deliveryCharge %></td>
                      <td><%= order.couponDetails.claimedAmount%></td>
                      <td><%= order.offerDiscount %>%</td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
        
              <% if (totalPages > 1 && totalOrders >= ITEMS_PER_PAGE) { %>
                <nav aria-label="Page navigation">
                  <ul class="pagination">
                    <% for (let i = 1; i <= totalPages; i++) { %>
                      <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                        <a class="page-link" href="/admin/salesReport?page=<%= i %>&sortBy=<%= sortBy %>&startDate=<%= startDate %>&endDate=<%= endDate %>"><%= i %></a>
                      </li>
                    <% } %>
                  </ul>
                </nav>
              <% } %>
            </div>
            <% } else { %>
              <div>
                <p>No transactions</p>
              </div>
            <% } %>
          </div>

