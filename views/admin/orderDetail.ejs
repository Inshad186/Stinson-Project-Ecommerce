                          <th>Variant Price</th>
                          <th>Quantity</th>
                          <th>Order Status</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% order.orderItems.forEach(function(item) { %>
                          <tr>
                            <td><%= item.variantName %></td>
                            <td><%= item.variantPrice %></td>
                            <td><%= item.quantity %></td>
                            <td>
                              <select class="form-control status-select" style="color: white; background-color: rgb(226, 55, 55);" data-order-id="<%= order._id %>" data-variant-id="<%= item.variantId._id %>" data-current-status="<%= item.orderStatus %>">
                                <%  
                                  const currentStatus = item.orderStatus;
                                  const nextStatuses = statusChanges[currentStatus] || []; 
                                %>    
                                <option value="<%= currentStatus %>" selected><%= currentStatus %></option>
                                <% nextStatuses.forEach(status => { %>
                                  <option value="<%= status %>"><%= status %></option>
                                <% }); %>
                              </select>                              
                            </td>
                          </tr>
                        <% }); %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
