                                                const descriptions = {
                                                    'Razorpay': 'Payment made via Razorpay',
                                                    'Refund': 'Refund processed',
                                                    'Purchase': 'Purchase made',
                                                    'Cancelled': 'Order cancelled'
                                                };
                                                const description = descriptions[transaction.transactionMethod] || "No description";
                                            %>
                                            <tr class="table-row transaction-row">
                                                <td class="table-wrapper wrapper-date">
                                                    <div class="wrapper-content">
                                                        <h5 class="table-heading">
                                                            <%= transaction.date.toLocaleDateString() %>
                                                        </h5>
                                                    </div>
                                                </td>
                                                <td class="table-wrapper wrapper-status">
                                                    <div class="table-wrapper-center">
                                                        <h5 class="table-heading">
                                                            <%= transaction.transactionMethod %>
                                                        </h5>
                                                    </div>
                                                </td>
                                                <td class="table-wrapper wrapper-amount">
                                                    <div class="table-wrapper-center">
                                                        <h5 class="table-heading">
                                                            <%= parseInt(transaction.amount) %>
                                                        </h5>
                                                    </div>
                                                </td>
                                                <td class="table-wrapper wrapper-description">
                                                    <div class="table-wrapper-center">
                                                        <h5 class="table-heading">
                                                            <%= description %>
                                                        </h5>
                                                    </div>
                                                </td>
                                            </tr>
                                            <% }); %>
                                        <% } else { %>
                                            <tr>
                                                <td colspan="4" class="table-wrapper wrapper-no-transactions">
                                                    <div class="wrapper-content">
                                                        <h5 class="heading">No transactions found</h5>
                                                    </div>
                                                </td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                                <% if (wallet && wallet.transactions && wallet.transactions.length > 0) { %>
                                    <div class="pagination">
                                        <button id="prevPage" onclick="loadTransactions(currentPage - 1)">Previous</button>
                                        <span id="pageIndicator"></span>
                                        <button id="nextPage" onclick="loadTransactions(currentPage + 1)">Next</button>
                                    </div>
                                <% } %>                                
                            </div>
                        </div>


