    document.querySelector('.close-btn').addEventListener('click', function () {
        document.getElementById('editAddressModal').style.display = 'none';
        document.getElementById('editAddressModal').classList.remove('active');
    });

    // Close the modal when clicking anywhere outside of it
    window.onclick = function (event) {
        if (event.target == document.getElementById('editAddressModal')) {
            document.getElementById('editAddressModal').style.display = 'none';
            document.getElementById('editAddressModal').classList.remove('active');
        }
    }

    // Handle form submission
    document.getElementById('editAddressForm').addEventListener('submit', async function (event) {
        event.preventDefault();
        const formData = new FormData(this);
        const addressId = formData.get('id');

        try {
            const response = await fetch(`/editAddress/${addressId}`, {
                method: 'PATCH',
                body: JSON.stringify(Object.fromEntries(formData)),
                headers: {
                    'Content-Type': 'application/json'
                }
            });

            if (response.ok) {
                const updatedAddress = await response.json();

                // Update the address details on the client side
                const addressElement = document.getElementById(`address-${addressId}`);
                addressElement.querySelector('.info-details p:nth-child(1)').textContent = updatedAddress.name;
                addressElement.querySelector('.info-details p:nth-child(2)').textContent = updatedAddress.mobile;
                addressElement.querySelector('.info-details p:nth-child(3)').textContent = updatedAddress.city;
                addressElement.querySelector('.info-details p:nth-child(4)').textContent = updatedAddress.pincode;
                addressElement.querySelector('.info-details p:nth-child(5)').textContent = updatedAddress.postOffice;
                addressElement.querySelector('.info-details p:nth-child(6)').textContent = updatedAddress.landMark;
                addressElement.querySelector('.info-details p:nth-child(7)').textContent = updatedAddress.state;

                // Close the modal
