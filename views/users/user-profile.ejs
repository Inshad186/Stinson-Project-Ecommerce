<script>
    function requestReturn(orderId, variantId, reason) {
    fetch('/returnOrder', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({ orderId: orderId, variantId: variantId, reason: reason })
    })
    .then(response => response.json())
    .then(data => {
        if (data.message) {
            alert(data.message);
            location.reload();
        } else if (data.error) {
            alert(data.error);
        }
    })
    .catch(error => console.error('Error:', error));
}

document.addEventListener('DOMContentLoaded', function () {
    function cancelOrder(orderId, variantId) {
        fetch('/cancelOrder', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ orderId: orderId, variantId: variantId })
        })
        .then(response => response.json())
        .then(data => {
            if (data.message) {
                alert(data.message);
                // Remove the canceled product from the DOM
                const productRow = document.querySelector(`[data-order-id="${orderId}"][data-variant-id="${variantId}"]`);
                if (productRow) {
                    productRow.remove();
                }
            } else if (data.error) {
                alert(data.error);
            }
        })
        .catch(error => console.error('Error:', error));
    }

    function showReturnModal(orderId, variantId) {
        Swal.fire({
            title: 'Return Reason',
            input: 'select',
            inputOptions: {
                'Poor Quality': 'Poor Quality',
                'Wrong Product': 'Wrong Product',
                'Damaged Product': 'Damaged Product',
                'Not as Described': 'Not as Described',
                'Received Too Late': 'Received Too Late',
                'Ordered by Mistake': 'Ordered by Mistake',
                'Other': 'Other'
            },
            inputPlaceholder: 'Select a reason',
            showCancelButton: true,
            inputValidator: (value) => {
                if (!value) {
                    return 'You need to select a reason!';
                }
            }
        }).then((result) => {
            if (result.isConfirmed) {
                requestReturn(orderId, variantId, result.value);
            }
        });
    }

    function returnOrder(orderId, variantId) {
        showReturnModal(orderId, variantId);
    }

    // Attach event listeners to action buttons
    const actionButtons = document.querySelectorAll('[id^="actionButton-"]');
    actionButtons.forEach(function (button) {
        button.addEventListener('click', function (event) {
            const action = this.getAttribute('data-action');
            const orderId = this.closest('tr').getAttribute('data-order-id');
            const variantId = this.getAttribute('id').split('-')[1];

            if (action === 'cancel') {
                cancelOrder(orderId, variantId);
            } else if (action === 'return') {
                returnOrder(orderId, variantId);
            }
        });
    });
});

</script>

