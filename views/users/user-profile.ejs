<!------------------------------- This is for Add New Address ------------------------------->

<script>

    document.getElementById("addressForm").addEventListener("submit", async function (event) {
        event.preventDefault();

        const name = document.getElementById("name").value;
        const phone = document.getElementById("phone").value;
        const city = document.getElementById("city").value;
        const landMark = document.getElementById("landMark").value;
        const postoffice = document.getElementById("postoffice").value;
        const pincode = document.getElementById("pincode").value;
        const state = document.getElementById("state").value;

        const nameRegex = /^[^\s][a-zA-Z\s]*[^\s]$/;
        const phoneRegex = /^(\+?\d{1,3}[- ]?)?(6|7|8|9)\d{9}$/;
        const cityRegex = /^[a-zA-Z\s]+$/;
        const landMarkRegex = /^[a-zA-Z0-9\s]+$/;
        const postofficeRegex = /^[a-zA-Z\s]+$/;
        const pincodeRegex = /^\d{6}$/;
        const stateRegex = /^[a-zA-Z\s]+$/;


        let isValid = true;

        if (!nameRegex.test(name)) {
            showError('name-error', "User Names can only contain letters and special characters.");
            isValid = false;
        } else {
            hideError('name-error');
        }

        if (!phoneRegex.test(phone)) {
            showError('phone-error', "Please enter a valid phone number.");
            isValid = false;
        } else {
            hideError('phone-error');
        }

        if (!cityRegex.test(city)) {
            showError('city-error', "City name can only contain letters.");
            isValid = false;
        } else {
            hideError('city-error');
        }

        if (!landMarkRegex.test(landMark)) {
            showError('landMark-error', "Landmark can only contain letters and numbers.");
            isValid = false;
        } else {
            hideError('landMark-error');
        }

        if (!postofficeRegex.test(postoffice)) {
            showError('postoffice-error', "Post Office name can only contain letters.");
            isValid = false;
        } else {
            hideError('postoffice-error');
        }

        if (!pincodeRegex.test(pincode)) {
            showError('pincode-error', "Please enter a valid pincode.");
            isValid = false;
        } else {
            hideError('pincode-error');
        }

        if (!stateRegex.test(state)) {
            showError('state-error', "State name can only contain letters.");
            isValid = false;
        } else {
            hideError('state-error');
        }


        const addressData = {
            name,
            phone,
            city,
            landMark,
            postoffice,
            pincode,
            state
        };


        if (!isValid) {
            return;
        }

        try {
            const response = await fetch('/insertAddress', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify(addressData)
            });

            const data = await response.json();

            if (response.ok) {
                await Swal.fire({
                    title: 'Success!',
                    text: 'Address added successfully!',
                    icon: 'success'
                });
            } else {
                await Swal.fire({
                    title: 'Error',
                    text: data.error || 'An unexpected error occurred.',
                    icon: 'error'
                });
            }
        } catch (error) {
            console.error('Error:', error);
            await Swal.fire({
                title: 'Error',
                text: 'An unexpected error occurred.',
                icon: 'error'
            });
        }
    });

    function showError(elementId, message) {
        const errorElement = document.getElementById(elementId);
        errorElement.innerText = message;
        errorElement.style.display = 'block';
    }

    function hideError(elementId) {
        const errorElement = document.getElementById(elementId);
        errorElement.style.display = 'none';
    }

</script>

<!------------------------------- This is for Change Password --------------------------------->

<script>
    // Define regex patterns
    const passwordRegex = /^(?=.*[a-zA-Z])(?=.*\d)[a-zA-Z\d]{6,}$/;

    document.getElementById('changePasswordForm').addEventListener('submit', async function (event) {
        event.preventDefault();

        const currentpass = document.getElementById('currentpass').value;
        const newPassword = document.getElementById('newPassword').value;
        const confirmPassword = document.getElementById('confirmPassword').value;

        // Clear previous error messages
        document.getElementById('currentpassError').textContent = '';
        document.getElementById('newPasswordError').textContent = '';
        document.getElementById('confirmPasswordError').textContent = '';

        let isValid = true;

        // Validate new password with regex
        if (!passwordRegex.test(newPassword)) {
            document.getElementById('newPasswordError').textContent = 'Password must be at least 6 characters long and include both letters and numbers.';
            isValid = false;
        }

        // Validate confirm password
        if (newPassword !== confirmPassword) {
            document.getElementById('confirmPasswordError').textContent = 'New password and confirm password must match.';
            isValid = false;
        }

        // Validate if current password and new password are the same
        if (currentpass === newPassword) {
            document.getElementById('newPasswordError').textContent = 'Current password and new password must be different.';
            isValid = false;
        }

        if (!isValid) return;

        // Submit the form to server
        try {
            const response = await fetch('/changePassword', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ currentpass, newPassword })
            });

            if (response.ok) {
                await Swal.fire({
                    title: 'Success',
                    text: 'Password updated successfully.',
                    icon: 'success'
                });
            } else {
                const errorMessage = await response.text();
                await Swal.fire({
                    title: 'Oops',
                    text: `Failed to update password: ${errorMessage}`,
                    icon: 'error'
                });
            }
        } catch (error) {
            console.error('Error:', error);
            await Swal.fire({
                title: 'Oops',
                text: 'An error occurred while updating password.',
                icon: 'error'
            });
        }
    });
</script>


