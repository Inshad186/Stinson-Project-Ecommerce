<script>
async function repayment(orderId, variantId) {
    try {
        const response = await fetch('/rePayment', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ orderId: orderId })
        });

        const data = await response.json();
        if (!response.ok || !data.razorpayOrder) {
            alert(data.message || "Re-payment failed");
            return;
        }

        const options = {
            "key": data.key,
            "amount": data.razorpayOrder.amount,
            "currency": data.razorpayOrder.currency,
            "name": "Your Store Name",
            "order_id": data.razorpayOrder.id,
            "handler": async function (response) {
                const paymentData = {
                    razorpay_order_id: response.razorpay_order_id,
                    razorpay_payment_id: response.razorpay_payment_id,
                    razorpay_signature: response.razorpay_signature,
                    order_id: orderId
                };

                try {
                    const res = await fetch('/payment-success', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify(paymentData)
                    });

                    const verifyData = await res.json();

                    if (verifyData.success) {
                        alert(verifyData.success || "Payment successful");
                        location.reload();
                    } else if (verifyData.error) {
                        alert(verifyData.error || "Payment verification failed");
                    }
                } catch (error) {
                    alert("An error occurred during payment verification.");
                }
            },
            "prefill": {
                "name": "Your Customer Name",
                "email": "customer@example.com",
                "contact": "9999999999"
            },
            "theme": {
                "color": "#3399cc"
            },
            "modal": {
                "ondismiss": function () {
                    alert("Payment was dismissed. Please try again.");
                }
            }
        };

        const rzp1 = new Razorpay(options);
        rzp1.open();
    } catch (error) {
        alert("An error occurred while initiating re-payment.");
    }
}
</script>

